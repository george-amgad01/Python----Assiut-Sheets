a,s,b,q,c = input().split()
if s == "+" :
    if int(a) + int(b) == int(c) :
        print("Yes")
    else :
        print(int(a) + int(b))
elif s == "-" :
    if int(a) - int(b) == int(c) :
        print("Yes")
    else:
        print(int(a) - int(b))
elif s == "*" :
    if int(a) * int(b) == int(c) :
        print("Yes")
    else :
        print(int(a) * int(b))
