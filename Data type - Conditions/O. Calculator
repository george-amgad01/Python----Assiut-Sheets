a= input()
if "+" in a :
    a=a.split("+")
    a[0] = int(a[0])
    a[1] = int(a[1])
    print(a[0]+a[1])
elif "-" in a :
    a=a.split("-")
    a[0] = int(a[0])
    a[1] = int(a[1])
    print(a[0]-a[1])
elif "*" in a :
    a=a.split("*")
    a[0] = int(a[0])
    a[1] = int(a[1])
    print(a[0]*a[1])
elif "/" in a :
    a=a.split("/")
    a[0] = int(a[0])
    a[1] = int(a[1])
    print(a[0]//a[1])
