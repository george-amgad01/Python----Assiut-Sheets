T = int(input())
for w in range (T) :
    N =int(input())
    arr = list(map(int,input().split()))
    for i in range (N) :   
        j= i 
        for x in range (N) :     
            if j == N - 1 :
                print(max(arr[x:]),end = " ")
                break
            else :
                print(max(arr[x: j+1]),end = " ")
                j+=1
    print()
