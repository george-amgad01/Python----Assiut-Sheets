T = int(input())
for w in range (T) :
    N =int(input())
    arr = list(map(int,input().split()))   # 7 4 9 8
    for i in range (N) :           # 0 1 2 3
        j= i 
        for x in range (N) :      # 0 1 2 3 
            if j == N - 1 :
                print(max(arr[x:]),end = " ")
                break
            else :
                print(max(arr[x: j+1]),end = " ")    # 0 1    1 2 
                j+=1
    print()
