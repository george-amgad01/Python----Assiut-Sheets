N =int(input())
arr = list(map(int,input().split()))
pos = neg = even = odd = 0
for i in arr :
    if i % 2 == 0 :
        even+=1
    else :
        odd+=1
    if i >0 :
        pos+=1
    if i < 0 :
        neg+=1
print(f'Even: {even}')
print(f'Odd: {odd}')
print(f'Positive: {pos}')
print(f'Negative: {neg}')
